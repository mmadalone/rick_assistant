#!/usr/bin/env zsh
# Autoloaded function: _rick_cmd_help
# Generated by Rick Assistant Autoload Framework

_rick_cmd_help() {
    local cmd="$1"
    
    if [[ -z "$cmd" ]]; then
      rick_list_commands
      echo ""
      echo "Type \"rick help <command>\" for command-specific help."
    else
      rick_get_command_help "$cmd"
    fi
    return 0
  }

